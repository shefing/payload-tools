{
  "name": "@shefing/quickfilter",
  "version": "1.0.75",
  "private": false,
  "bugs": "https://github.com/shefing/payload-tools/issues",
  "repository": "https://github.com/shefing/payload-tools",
  "license": "MIT",
  "author": "shefing",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./QuickFilter": {
      "import": "./dist/QuickFilter.js",
      "require": "./dist/QuickFilter.js"
    },
    "./nav": {
      "import": "./dist/nav/index.js",
      "require": "./dist/nav/index.js"
    },
    "./Dashboard": {
      "import": "./dist/Dashboard/index.js",
      "require": "./dist/Dashboard/index.js"
    }
  },
  "scripts": {
    "build": "pnpm copyfiles && pnpm build:swc && pnpm build:types",
    "build:swc": "swc ./src -d ./dist --config-file .swcrc",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "clean": "rimraf dist && rimraf tsconfig.tsbuildinfo",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png,json}\" dist/",
    "lint": "eslint src",
    "lint:fix": "eslint --fix --ext .ts,.tsx src",
    "prepublishOnly": "pnpm clean && pnpm build",
    "reinstall": "rm -rf node_modules .next && rm -rf .next && rm pnpm-lock.yaml  && pnpm install"
  },
  "devDependencies": {
    "@payloadcms/translations": "3.56.0",
    "@payloadcms/next": "3.56.0",
    "@payloadcms/ui": "3.56.0",
    "@swc/cli": "^0.1.65",
    "@swc/core": "^1.6.3",
    "@types/react": "19.1.12",
    "@types/react-dom": "19.1.9",
    "class-variance-authority": "^0.7.1",
    "copyfiles": "^2.4.1",
    "lucide-react": "^0.475.0",
    "postcss": "^8.5.2",
    "rimraf": "^5.0.5",
    "tailwind-merge": "^3.0.1",
    "tailwindcss": "3.4.17",
    "typescript": "5.7.3",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "next": "15.4.4",
    "payload": "3.56.0"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.2.3",
    "@types/lodash": "^4.14.202",
    "@types/react": "19.0.7",
    "@types/react-dom": "19.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.0",
    "cmdk": "^1.0.4",
    "date-fns": "^3.6.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.475.0",
    "qs-esm": "7.0.2",
    "react-day-picker": "^9.8.0",
    "tailwind-merge": "^3.0.1",
    "tailwindcss": "3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss-scoped-preflight": "^3.4.10",
    "typescript": "5.7.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
