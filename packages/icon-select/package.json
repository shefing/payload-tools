{
  "name": "@shefing/icon-select",
  "version": "1.0.2",
  "private": false,
  "bugs": "https://github.com/shefing/payload-tools/issues",
  "repository": "https://github.com/shefing/payload-tools",
  "license": "MIT",
  "author": "shefing",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./selectIcons": {
      "import": "./dist/selectIcons.js",
      "require": "./dist/selectIcons.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm copyfiles && pnpm build:swc && pnpm build:types",
    "build:swc": "swc ./src -d ./dist --config-file .swcrc",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "clean": "rimraf dist && rimraf tsconfig.tsbuildinfo",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png,json}\" dist/",
    "lint": "eslint src",
    "lint:fix": "eslint --fix --ext .ts,.tsx src",
    "prepublishOnly": "pnpm clean && pnpm build",
    "reinstall": "rm -rf node_modules .next && rm -rf .next && rm pnpm-lock.yaml  && pnpm install"
  },
  "devDependencies": {
    "payload": "3.56.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "next": "15.0.4",
    "@payloadcms/ui": "3.56.0",
    "typescript": "^5.5.2",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.1",
    "tailwindcss": "^3.4.14"
  },
  "dependencies": {
    "lucide-react": "^0.394.0",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.0",
    "class-variance-authority": "^0.7.0",
    "@radix-ui/react-icons": "^1.3.1",
    "@radix-ui/react-popover": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.3",
    "cmdk": "1.0.0",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss-scoped-preflight": "^3.2.8",
    "flowbite-react-icons": "^1.0.8"
  },
  "publishConfig": {
    "access": "public"
  }
}
