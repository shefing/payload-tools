{
  "name": "@shefing/authors-info",
  "version": "1.0.12",
  "private": false,
  "bugs": "https://github.com/shefing/payload-tools/issues",
  "repository": "https://github.com/shefing/payload-tools",
  "license": "MIT",
  "author": "shefing",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./types": {
      "import": "./dist/exports/types.js",
      "require": "./dist/exports/types.js"
    },
    "./client": {
      "import": "./dist/exports/client.js",
      "require": "./dist/exports/client.js"
    }
  },
  "scripts": {
    "build": "pnpm copyfiles && pnpm build:swc && pnpm build:types",
    "build:swc": "swc ./src -d ./dist --config-file .swcrc",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "clean": "rimraf dist && rimraf tsconfig.tsbuildinfo",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png,json}\" dist/",
    "lint": "eslint src",
    "lint:fix": "eslint --fix --ext .ts,.tsx src",
    "prepublishOnly": "pnpm clean && pnpm build",
    "reinstall": "rm -rf node_modules .next && rm -rf .next && rm pnpm-lock.yaml  && pnpm install"
  },
  "devDependencies": {
    "payload": "3.35.1",
    "typescript": "^5.7.3",
    "@payloadcms/ui": "3.35.1",
    "@types/react": "19.0.7",
    "@types/react-dom": "19.0.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "moment": "^2.30.1",
    "rimraf": "^5.0.5",
    "copyfiles": "^2.4.1"
  },
  "dependencies": {
    "moment": "^2.30.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@payloadcms/ui": "3.35.1",
    "payload": "3.35.1",
    "i18next": "^23.0.0"
  }
}
